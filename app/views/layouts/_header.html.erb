<header>
  <div>
    <nav>
      <ul>
        <% if logged_in? %>
          <li>
			<ul>
				<%= link_to "Users", users_path %>
			</ul>
		  </li>
          <li>
            <ul>
              <li><%= link_to "Profile", current_user %></li>
              <li><%= link_to "Settings",edit_user_path(current_user) %></li>
              <li>
                <%= link_to "Log out", sign_out_path, method: "delete" %>
              </li>
            </ul>
          </li>
        <% else %>
          <li>
			<ul>
				<%= link_to "Log in", root_path %>
			</ul>
			<ul>
				<%= image_tag "farda.jpg" %>
			</ul>
		  </li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>