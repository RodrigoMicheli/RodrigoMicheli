<!DOCTYPE html>
<html>
	  <head>
	   <tr>
			<td>
				<%= video_tag "xeque_mate.mp4" %>
			</td>
		</tr>
	  </head>
	  <body>
		<%= form_for :session, url: sign_in_path do |f| %>
		  <div class="field">
			<%= f.label :email %><br />
			<%= f.email_field :email, autofocus: true %>
		  </div>

		  <div class="field">
			<%= f.label :password %><br />
			<%= f.password_field :password, autocomplete: "off" %>
		  </div>
		  <div class="actions">
			<%= f.submit "Fazer Login" %>
		  </div>
		<p>New user? <%= link_to "Sign up now!", new_user_path %></p>
		<% end %>
	</body>
</html>